{
  "name": "compute_tool",
  "version": "0.7.0",
  "description": "Compute tool for multi computer",
  "main": "main/main.js",
  "bin": {
    "ctn": "./build/node-build/app.exe"
  },
  "scripts": {
    "dev": "ts-node scripts/dev-server.ts",
    "web": "ts-node scripts/web.ts && cd ./build/renderer && http-server -p 80",
    "express": "ts-node scripts/express.ts && cd ./src/server && ts-node .",
    "node": "ts-node src/node/main.ts",
    "share": "ts-node scripts/share.ts",
    "build": "ts-node scripts/build.ts && electron-builder",
    "build:web": "ts-node scripts/build.ts",
    "build:node": "ts-node scripts/node_builder.ts",
    "build:node-pkg": "ts-node scripts/node_builder.ts --pkg",
    "build:server": "ts-node scripts/server_builder.ts",
    "build:dir": "ts-node scripts/before_build.ts && electron-builder --dir",
    "build:win": "ts-node scripts/before_build.ts && electron-builder --win",
    "build:mac": "ts-node scripts/before_build.ts && electron-builder --mac",
    "build:linux": "ts-node scripts/before_build.ts && electron-builder --linux",
    "docker:node": "docker_node.sh",
    "docker:server": "docker_server.sh",
    "pkg": "ts-node scripts/program_build.ts",
    "test": "jest"
  },
  "devDependencies": {
    "@iconify-json/fa": "^1.2.1",
    "@iconify-json/mdi": "^1.2.3",
    "@mdi/font": "^7.4.47",
    "@types/bcrypt": "^5.0.2",
    "@types/byte-size": "^8.1.2",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/tcp-port-used": "^1.0.4",
    "@types/vue-markdown": "^2.2.4",
    "@types/ws": "^8.5.14",
    "@vitejs/plugin-vue": "^4.4.1",
    "@vue/compiler-sfc": "^3.5.13",
    "chalk": "^4.1.2",
    "chokidar": "^3.5.3",
    "electron": "^32.1.2",
    "electron-builder": "^25.1.6",
    "electron-installer-snap": "^5.2.0",
    "sass": "^1.84.0",
    "sass-loader": "^16.0.4",
    "ts-jest": "^29.3.1",
    "typescript": "^5.2.2",
    "unplugin-icons": "^22.0.0",
    "unplugin-vue-components": "^28.0.0",
    "vite": "^4.5.0"
  },
  "dependencies": {
    "@codemirror/language-data": "^6.5.1",
    "@codemirror/legacy-modes": "^6.4.2",
    "@intlify/devtools-types": "^11.1.1",
    "bcrypt": "^5.1.1",
    "byte-size": "^9.0.1",
    "codemirror": "^6.0.1",
    "express": "^4.21.2",
    "expressionparser": "^1.1.5",
    "jest": "^29.7.0",
    "jsonwebtoken": "^9.0.2",
    "keypair": "^1.0.4",
    "lua-in-js": "file:plugins/lua-in-js",
    "mitt": "^3.0.1",
    "pkg": "^5.8.1",
    "systeminformation": "^5.25.11",
    "tcp-port-used": "^1.0.2",
    "tree-kill": "^1.2.2",
    "uuid": "^11.0.5",
    "vue": "^3.3.8",
    "vue-codemirror": "^6.1.1",
    "vue-i18n": "^10.0.5",
    "vue-markdown": "^2.2.4",
    "vuetify": "^3.7.11",
    "ws": "^8.18.0"
  }
}
